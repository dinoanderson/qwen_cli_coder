# Qwen/DashScope Configuration
QWEN_API_KEY=your_qwen_api_key_here
# Alternative: Use DASHSCOPE_API_KEY if you prefer
# DASHSCOPE_API_KEY=your_dashscope_api_key_here

# Optional: Override Qwen base URL (defaults to international endpoint)
# QWEN_BASE_URL=https://dashscope-intl.aliyuncs.com/api/v1

# Optional: Enable thinking mode for Qwen (defaults to false)
# QWEN_ENABLE_THINKING=true

# Google OAuth Configuration (for Vertex AI authentication if needed)
# GOOGLE_OAUTH_CLIENT_ID=your_google_oauth_client_id_here
# GOOGLE_OAUTH_CLIENT_SECRET=your_google_oauth_client_secret_here

# CLI Version (optional)
# CLI_VERSION=1.0.0

# MCP Server Configuration
# Note: MCP servers are configured in .gemini/settings.json, not in .env
# This section is for reference only

# Example MCP configuration for .gemini/settings.json:
# {
#   "mcpServers": {
#     "duckduckgo": {
#       "command": "npx",
#       "args": ["-y", "@nickclyde/duckduckgo-mcp-server"],
#       "timeout": 30000
#     }
#   }
# }

# Optional: SerpAPI key for enhanced search results (if using SerpAPI-enabled MCP)
# SERPAPI_KEY=your_serpapi_key_here